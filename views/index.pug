extends partials/template

block variables
    - var pageTitle = "About";

block content
    main
        section.banner-area
            .container
                .row.fullscreen.align-items-center.justify-content-between
                    .col-lg-6.col-md-6.banner-left.align-self-start
                        article.top-part
                            h1.text-uppercase I&apos;m Zidik
                            h2 Web Developer
                            p
                                | It is my personal website. Here you can find information about me and my projects.
                            aside.github-project-link
                                a(href='https://github.com/Zidikk/WebDev')
                                    i.fa(class='fa-https://github.com/Zidikk/WebDev')
                            a.primary-btn(href='#') Get Started
                    .wrapper
                        div One
                        div Two
                        div Three
                        div Four
                        div Five
                    .table-info
                        table
                            tr
                                th Month
                                th Savings
                            tr
                                td January
                                td $100
                            tr
                                td February
                                td $80
                    .col-lg-6.col-md-6.banner-right.d-flex
                        img.img-fluid.hide-on-phone(src='img/me.jpg' alt='')